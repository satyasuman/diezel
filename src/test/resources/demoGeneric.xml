<?xml version="1.0" encoding="UTF-8"?>
<diezel xmlns="http://diezel.ericaro.net/2.0.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<package>net.ericaro.diezel.xml</package>
	<name>DemoBuilder</name>
	<expression>a,b, build</expression>
	<transitions>
		<transition name="a">
			<signature>with(Class&lt;T> type)</signature>
			<capture name="T"/>
		</transition>
		<transition name="b">
			<signature>instance(T b)</signature>
			<drop name="T"/>
		</transition>
		<transition name="build">
			<signature>build()</signature>
		</transition>
	</transitions>
</diezel>
