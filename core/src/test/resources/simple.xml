<?xml version="1.0" encoding="UTF-8"?>
<diezel xmlns="http://diezel.ericaro.net/2.0.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<package>net.ericaro.diezel.xml</package>
	<!-- <header>My HEADER</header> -->
	<name>MyGuide</name>
	<capture name="U" extends="String" />
	<expression>(a|b), c,d</expression>
	<states>
		<state path="a.c">Middle</state>
	</states>
	<transitions>
		<transition name="a">
			<capture name="W" extends="Object" />
			<javadoc>Hello there</javadoc>
			<return>void</return>
			<signature>trans_a()</signature>
		</transition>
		<transition name="b">
			<capture name="W" extends="Object" />
			<javadoc>Hello there</javadoc>
			<return>void</return>
			<signature>trans_b()</signature>
		</transition>
		<transition name="c">
			<drop name="W" extends="Object" />
			<javadoc>Hello there in C</javadoc>
			<return>void</return>
			<signature>withC(float f)</signature>
		</transition>
		<transition name="d">
			<javadoc>Hello there in D</javadoc>
			<return>void</return>
			<signature>build()</signature>
		</transition>
	</transitions>
</diezel>
